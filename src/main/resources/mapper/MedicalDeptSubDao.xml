<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hospital.patient.wx.api.db.dao.MedicalDeptSubDao">

    <select id="selectAllSubMedicalDeptSub" parameterType="java.lang.Integer" resultType="java.util.Map">
        SELECT "id","name"
        FROM HOSPITAL.MEDICAL_DEPT_SUB
        WHERE dept_id = ${medicalId}
    </select>
</mapper>
